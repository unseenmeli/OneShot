{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ]
    ],
    "name": "OneShot",
    "slug": "OneShot",
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select app logos"
      },
      "bundleIdentifier": "com.anonymous.OneShot"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE"
      ]
    }
  }
}
